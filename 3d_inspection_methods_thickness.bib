@ARTICLE{Dai2024,
	author = {Dai, Zijie and Lai, Longhao and Pan, Eryi and Ye, Yunxia and Chen, Mingyang and Huang, Zhiming},
	title = {Tunable and Broadband Hybrid Plasmonic Terahertz Waveguide Based on 3D Dirac Semimetals; [基 于 狄 拉 克 半 金 属 的 混 合 等 离 子 体 宽 带可 调 谐 太 赫 兹 波 导]},
	year = {2024},
	journal = {Zhongguo Jiguang/Chinese Journal of Lasers},
