}

@ARTICLE{Penney1990707,
	author = {Penney, Bill C. and King, Michael A. and Glick, Stephen J.},
	title = {Restoration of combined conjugate images in SPECT: Comparison of a new Wiener filter and the image-dependent Metz filter},
	year = {1990},
	journal = {IEEE Transactions on Nuclear Science},
	volume = {37},
	number = {2},
	pages = {707 – 712},
	doi = {10.1109/23.106702},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-0025414328&doi=10.1109%2f23.106702&partnerID=40&md5=3215316a239105c79b75a6643be57fc3},
	affiliations = {Department of Nuclear Medicine, University of Massachusetts Medical Center, Worcester, MA, 01655, United States},
	abstract = {This study applies two image-dependent restoration filters to projection image sets obtained with single photon emission computed tomography (SPECT). Wiener and Metz restoration of combined conjugate views are compared to: 1)each other, 2)Wiener restoration of individual projection images, and 3)to one-dimensional (1D) Butterworth smoothing. The combined view restoration filters adapt to the average thickness of the object by estimating a modulation transfer function (MTF) for that thickness. Simulated Tc-99m liver-spleen studies with randomly placed “cold spot tumors”, a projector which accounts for the spatially variant blurring in SPECT, and a Poisson noise generator are used to compute simulated projection image sets. These sets are filtered and reconstructed using Tanaka's method of intrinsic attenuation correction. Cold spot contrast, automated receiver operator characteristic (ROC) analysis of cold spot detectability, and the normalized mean squared error (NMSE) are used to compare the four processing methods. Little difference is noted between the three restoration methods. However, the restoration filters all yield noticeably better ROC and NMSE results than the ID smoothing. Inspection of the three-dimensional (3D) MTF derived from reconstructions of point sources indicates that the restoration filters substantially reduce the low-frequency degradation which is primarily due to scatter. Copyright © 1990 by The Institute of Electrical and Electronics Engineers, Inc.},
	keywords = {Image Processing--Enhancement; Mathematical Techniques--Transfer Functions; Photons--Emission; Butterworth Smoothing; Metz Filter; Single-Photon-Emission Computed Tomography (SPECT); Wiener Filter; Computerized Tomography},
	issn = {00189499},
	language = {English},
	abbrev_source_title = {IEEE Trans Nucl Sci},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 10}
}

