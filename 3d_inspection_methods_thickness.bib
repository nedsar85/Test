@CONFERENCE{Aldrin2016,
	author = {Aldrin, John C. and Sabbagh, Harold A. and Zhao, Liming and Sabbagh, Elias and Murphy, R. Kim and Keiser, Mark and Flores-Lamb, Jennifer and Forsyth, David S. and Motes, Doyle and Lindgren, Eric A. and Mooers, Ryan},
	title = {Model-based inverse methods for sizing cracks of varying shape and location in bolt-hole eddy current (BHEC) inspections},
	year = {2016},
	journal = {AIP Conference Proceedings},
	volume = {1706},
	doi = {10.1063/1.4940557},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84984568792&doi=10.1063%2f1.4940557&partnerID=40&md5=86b8bd1a20fab9991a383056303f95b6},
	affiliations = {Air Force Research Laboratory (AFRL/RXCA), Wright-Patterson AFB, 45433, OH, United States; Computational Tools, 4275 Chatham Avenue, Gurnee, 60031, IL, United States; Victor Technologies LLC, Bloomington, 47401, IN, United States; TRI/Austin, Austin, 78746, TX, United States},
	abstract = {A comprehensive approach is presented to perform model-based inversion of crack characteristics using bolt hole eddy current (BHEC) techniques. Data was acquired for a wide range of crack sizes and shapes, including mid-bore, corner and through-thickness crack types, and from both standard eddy current hardware and a prototype BHEC system with z-axis position encoding. Signal processing algorithms were developed to process and extract features from the 2D data sets, and inversion algorithms using VIC-3D generated surrogate models were used for inverting crack size. New model results are presented, which now address the effect of having a corner crack at an edge and a through crack adjacent to two edges. A two-step inversion process was implemented that first evaluates the material layer thickness, crack type and location, in order to select the most appropriate VIC-3D surrogate model for subsequent crack sizing inversion step. Inversion results for select mid-bore, through and corner crack specimens are presented where sizing performance was found to be satisfactory in general, but also depend on the size and location of the flaw. © 2016 AIP Publishing LLC.},
